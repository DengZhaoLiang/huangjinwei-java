<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.huangjinwei.mapper.OrderSnMapper">
    
    <select id="existsByOrderSn" resultType="com.huangjinwei.model.OrderSn">
        select *
        from `order_sn`
        where `no` = '${orderSn}'
    </select>
</mapper>